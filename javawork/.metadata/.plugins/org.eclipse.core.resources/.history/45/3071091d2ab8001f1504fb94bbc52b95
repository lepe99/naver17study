package day1212;

public class Ex2Operator {

	public static void main(String[] args) {
		// 산술 연산자 + - * / %
		
		int a = 10;
		int b = 10;
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b); // 몫 (묵시적 형변환에 의해 결과가 int 타입으로 출력되기 때문)
		System.out.println((double)a / b);
		System.out.println(a % b); // 나머지 
	}

}
