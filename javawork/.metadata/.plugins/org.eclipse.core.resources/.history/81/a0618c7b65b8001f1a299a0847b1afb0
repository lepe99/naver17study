package boj;

import java.util.Scanner;

public class Boj2a07 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] arr = new int[6];
		for(int i=0; i<3; i++) {
			int k = sc.nextInt();
			arr[k] += 1;
			
		}
		sc.close();
		
		System.out.println(arr[3]);
	}

}
