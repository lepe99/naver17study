package boj;

import java.util.Arrays;
import java.util.Scanner;

public class Boj2a07 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] arr = new int[7];
		for(int i=0; i<3; i++) {
			int k = sc.nextInt();
			arr[k] += 1;
		}
		sc.close();
		
		int ans;
		int max;
		
		for(int i=1; i<=7; i++) {
			if(arr[i] == 3)
				ans = 10000 + 1000 * i;
			else if(arr[i] == 2)
				ans = 1000 + 100 * i;
			
		}
		
		int val;
		if (ans[3] = 1)
			System.out.println(10000 + 1000 * );
			
		
			
	}

}
