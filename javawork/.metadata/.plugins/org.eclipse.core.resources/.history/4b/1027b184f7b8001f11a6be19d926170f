package day1213;

public class Ex6While {

	public static void main(String[] args) {
		int a = 65;
		
		while (a < 90) {
//			System.out.printf("%c", a++);
			System.out.print((char) a++);
		}
		
		System.out.println();
		char b ='a';
		//do-while 이용해 a~z 출력
		do {
			System.out.println(b++);
		} while(b == a + 27);
	}

}
